
@{
    ViewData["Title"] = "EditTeam";
    Layout = null;
}
@model RendERA.DB.ViewModels.MasterCategoryVM

<h2 style="padding: 8px; color:forestgreen">Edit Category / Plugin</h2>
<form enctype="multipart/form-data" asp-action="Edit">
    <div class="row">
        <div class="col-md-12">
            <div class="form-group">
                <input asp-for="Id" hidden />
                <label asp-for="Name" style="color:forestgreen" class="col-md-2 control-label"></label>
                <input asp-for="Name" class="col-md-4 msgForm" />
                <span asp-validation-for="Name" class="text-danger"></span>
            </div>
        </div>
    </div>
   
    
    <div class="form-group">
        <input style="padding: 8px;" class="btn bt-danger right" type="submit" value="submit" />
        <a asp-action="Index"><button style="padding: 8px; color:forestgreen" type="button"><i class=""></i> Back to List</button></a>
    </div>
</form>
<div class="js-fake-scroll"></div>

