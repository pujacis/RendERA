@model RendERA.DB.ViewModels.MasterCategoryVM

<h3 style="padding: 8px; color:forestgreen">Create Category / Plugin</h3>
<form enctype="multipart/form-data" asp-action="Create">
    <div class="row">
        <div class="col-md-12">
            <div class="form-group">
                <label asp-for="Name" class="col-md-2 control-label" style="color:forestgreen"></label>
                <input asp-for="Name" class="col-md-4 msgForm" required />
                <span asp-validation-for="Name" class="text-danger"></span>
            </div>
        </div>
    </div>
    <div class="form-group">
        <input style="padding: 8px;" class="btn bt-danger right" type="submit" value="submit" />
        <a asp-action="Index"><button style="padding: 8px;" type="button"><i class=""></i> Back to List</button></a>
    </div>
</form>
<div class="js-fake-scroll"></div>

