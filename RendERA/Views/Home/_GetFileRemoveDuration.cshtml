
@{
    ViewData["Title"] = "_GetFileRemoveDuration";
    Layout = null;
}
@model RendERA.DB.ViewModels.ConfigrationSettingsVM
    <form asp-controller="Home" asp-action="InsertFileRemoveDuration" method="post" enctype="multipart/form-data">
        <input asp-for="Id" min="0" max="99999" id="ParameterName" hidden class="msgForm" />
        <div class="form-group row">
            <label class=" col-sm-4  control-label">Pro</label>
            <div class="col-sm-4">
                <input asp-for="Pro" min="0" max="99999" id="ParameterName" class="msgForm" />
                <span asp-validation-for="Pro" class="text-danger"></span>
            </div>
        </div>
        <div class="form-group row">
            <label class=" col-sm-4  control-label">Beginner</label>
            <div class="col-sm-4">
                <input asp-for="Beginner" min="0" max="99999" id="ParameterName" class="msgForm" />
                <span asp-validation-for="Beginner" class="text-danger"></span>
            </div>
        </div>
        <div class="form-group row">
            <label class=" col-sm-4  control-label">Expert</label>
            <div class="col-sm-4">
                <input asp-for="Expert" min="0" max="99999" id="ParameterName" class="msgForm" />
                <span asp-validation-for="Expert" class="text-danger"></span>
            </div>
        </div>
        <input type="submit" style="padding:12px" class="btn btn-primary right" id="uploadButton" value="set">
    </form>
